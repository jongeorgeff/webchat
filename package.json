{
	"name": "webchat",
	"description": "Exercise to create a chat application with React, Express, and MongoDB.",
	"version": "0.0.1",
	"license": "MIT",
	"homepage": "https://github.com/jongeorgeff/webchat#readme",
	"bugs": {
		"url": "https://github.com/jongeorgeff/webchat/issues"
	},
	"author": {
		"name": "Jonathan Georgeff",
		"url": "https://github.com/jongeorgeff"
	},
	"main": "server/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/jongeorgeff/webchat.git"
	},
	"private": true,
	"scripts": {
		"start": "node .",
		"start:dev": "nodenv .",
		"build": "webpack --mode production",
		"build:dev": "webpack --mode development",
		"lint:client": "eslint -c client.eslintrc.js client/src/**/*.js client/src/**/*.jsx",
		"lint:scss": "npx stylelint ./client/src/**/*.scss",
		"lint:server": "eslint -c server.eslintrc.js server/**/*.js",
		"test": "npm run test:client && npm run test:server",
		"test:client": "jest --verbose --env=jsdom ./client/src",
		"test:server": "TOKEN_SECRET=testtoken jest --verbose ./server"
	},
	"dependencies": {
		"async": "^3.2.4",
		"cookie-parser": "^1.4.6",
		"ejs": "^3.1.8",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"jsonwebtoken": "^9.0.0",
		"mongoose": "^6.8.4",
		"websocket": "^1.0.34",
		"ws": "^8.12.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.20.7",
		"@babel/core": "^7.20.12",
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/runtime": "^7.20.13",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"babel-jest": "^29.3.1",
		"babel-loader": "^9.1.2",
		"commonmark": "^0.30.0",
		"css-loader": "^6.7.3",
		"eslint": "^8.33.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"i18next": "^22.4.9",
		"i18next-http-backend": "^2.1.1",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.4.2",
		"jsdom": "^21.1.0",
		"node-env-run": "^4.0.2",
		"node-sass": "^8.0.0",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^12.1.5",
		"react-test-renderer": "^18.2.0",
		"sass-loader": "^13.2.0",
		"style-loader": "^3.3.1",
		"stylelint": "^14.16.1",
